<%-- Typescript Interface Template --%>
<%@ val config: com.github.scala2ts.configuration.Configuration %>
<%@ val interface: com.github.scala2ts.model.Typescript.InterfaceDeclaration %>
<% val tpeArgs = interface.typeParams.mkString(", ") %>
export interface ${interface.name}#if(interface.typeParams.nonEmpty)<${tpeArgs}> {#else {#end

#for(field <- interface.fields)
  ${unescape(capture(render("field.ssp", Map("config" -> config, "member" -> field))).trim)}
#end
}